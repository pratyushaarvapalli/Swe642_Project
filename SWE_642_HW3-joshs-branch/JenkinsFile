pipeline {
    agent any
    stages {
        stage('git repo & clean') {
            steps {
               bat "rmdir  /s /q Swe642_Project"
                bat "git clone https://github.com/pratyushaarvapalli/Swe642_Project.git"
                bat "mvn clean -f Swe642_Project"
            }
        }
        stage('install') {
            steps {
                bat "mvn install -f Swe642_Project"
            }
        }
        stage('test') {
            steps {
                bat "mvn test -f Swe642_Project"
            }
        }
        stage('package') {
            steps {
                bat "mvn package -f Swe642_Project"
            }
        }
    }
}
