pipeline {
    agent any 
    stages {
        stage('git clean & repo') {
            steps {
                bat "rmdir  /s /q Swe642_Project"
                bat "git clone https://github.com/pratyushaarvapalli/Swe642_Project.git"
                bat "cd SWE_642_HW3-joshs-branch\swe642-frontend"
                echo 'Stage 1' 
                
            }
        }
        stage('Compile') {
            steps {
                bat "npm install"
                bat "ng build --prod"
                bat "ng serve"
            }
        }
        stage('Running') {
            steps {
                echo 'Running the code' 
            }
        }
        
        
    }
}
